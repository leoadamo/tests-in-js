{
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true
	},
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"settings": {
		"import/resolver": {
			"alias": {
				"map": [
					["@config", "./src/config"],
					["@lib", "./src/lib"],
					["@utils", "./src/utils"]
				],
				"extensions": [".js", ".json"]
			}
		}
	},
	"rules": {}
}
